    <PackageProjectUrl>https://github.com/the-dharex/MarkovLibrary.git</PackageProjectUrl>
    <PackageTags>markov;probabilistic;ai;text;nlp;dotnet</PackageTags>
  </PropertyGroup>

</Project>
